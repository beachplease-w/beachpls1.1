<div class="container">
  <div class="new_team">
    <%= simple_form_for @team_player, url: team_players_path do |f| %>
      <%= f.input :user, label: "Partenaire", collection: User.all, label_method: :last_name %>
      <%= f.submit "Créer l'équipe" %>
    <% end %>
    <br>
    <%= link_to "Retour", user_path(current_user.id) %>
  </div>
</div>
